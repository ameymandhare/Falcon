@model IList<Falcon.Entity.Prospect.ProspectStudentModel>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}

@section Styles{
    <link rel="stylesheet" href="~/Content/vendor/bootstrap/css/bootstrap.min.css">
    <link href="~/Content/vendor/fonts/circular-std/style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/libs/css/style.css">
    <link rel="stylesheet" href="~/Content/vendor/fonts/fontawesome/css/fontawesome-all.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/datatables/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/datatables/css/buttons.bootstrap4.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/datatables/css/select.bootstrap4.css">
    <link rel="stylesheet" type="text/css" href="~/Content/vendor/datatables/css/fixedHeader.bootstrap4.css">
}

<!-- ============================================================== -->
<!-- wrapper  -->
<!-- ============================================================== -->

    <div class="container-fluid dashboard-content ">
        <!-- ============================================================== -->
        <!-- pageheader  -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-header">
                    <h2 class="pageheader-title">Prospect Student List</h2>
                    <div class="page-breadcrumb">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item">@Html.ActionLink("Dashboard", "Home", "Dashboard")</li>
                                <li class="breadcrumb-item active" aria-current="page">Prospect Student List</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end pageheader  -->
        <!-- ============================================================== -->
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 p-0">
            <div class="card">
                <h5 class="card-header hidden"></h5>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered first">
                            <thead>
                                <tr>
                                    <th class="hidden">Id</th>
                                    <th>ApplicationNumber</th>
                                    <th>Name</th>
                                    <th>ApplicationDate</th>
                                    <th>AdmissionStatus</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td class="hidden">@item.Id</td>
                                        <td>@item.ApplicationNumber</td>
                                        <td>@item.Name</td>
                                        <td>@item.ApplicationDate</td>
                                        <td>@item.AdmissionStatus</td>
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "badge badge-pill badge-primary" })
                                            @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "badge badge-pill badge-primary" })
                                            @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "badge badge-pill badge-primary" })

                                        </td>
                                    </tr>
                                }

                            </tbody>
                            <tfoot>
                                <tr>
                                    <th class="hidden">Id</th>
                                    <th>ApplicationNumber</th>
                                    <th>Name</th>
                                    <th>ApplicationDate</th>
                                    <th>AdmissionStatus</th>
                                    <th></th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <p>
            @Html.ActionLink("Create New", "Create", null, new { @class = "badge badge-pill badge-primary" })
            @Html.ActionLink("Back", "Home", "Dashboard", null, new { @class = "badge badge-pill badge-primary" })
        </p>
    </div>

<!-- ============================================================== -->
<!-- end wrapper  -->
<!-- ============================================================== -->

@section Scripts {
    <script src="~/Content/vendor/jquery/jquery-3.3.1.min.js"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <script src="~/Content/vendor/slimscroll/jquery.slimscroll.js"></script>
    <script src="~/Content/vendor/multi-select/js/jquery.multi-select.js"></script>
    <script src="~/Content/libs/js/main-js.js"></script>
    <script src="~/Content/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/vendor/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/vendor/datatables/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/vendor/datatables/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Content/vendor/datatables/js/data-table.js"></script>
    <script src="~/Content/vendor/datatables/js/jszip.min.js"></script>
    <script src="~/Content/vendor/datatables/js/pdfmake.min.js"></script>
    <script src="~/Content/vendor/datatables/js/vfs_fonts.js"></script>
    <script src="~/Content/vendor/datatables/js/buttons.html5.min.js"></script>
    <script src="~/Content/vendor/datatables/js/buttons.print.min.js"></script>
    <script src="~/Content/vendor/datatables/js/buttons.colVis.min.js"></script>
    <script src="~/Content/vendor/datatables/js/dataTables.rowGroup.min.js"></script>
    <script src="~/Content/vendor/datatables/js/dataTables.select.min.js"></script>
    <script src="~/Content/vendor/datatables/js/dataTables.fixedHeader.min.js"></script>
}